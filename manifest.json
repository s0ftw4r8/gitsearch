{
  "manifest_version": 2,
  "name": "GitSearch",
  "version": "1.0",
  "author": "HolyNkona",
  "icons": {
    "16": "images/icon.png",
    "32": "images/icon.png",
    "48": "images/icon.png",
    "64": "images/icon.png",
    "128": "images/icon.png"
  },
  "background": {
    "scripts": [
      "popup/jquery.js"
    ],
    "persistent": false
  },
  "permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "browser_action": {
    "default_icon": {
      "16": "images/icon.png",
      "32": "images/icon.png",
      "48": "images/icon.png",
      "64": "images/icon.png",
      "128": "images/icon.png"
    },
    "default_title": "GitSearch",
    "default_popup": "popup/popup.html"
  }
}